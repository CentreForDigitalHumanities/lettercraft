{
  "name": "lettercraft",
  "scripts": {
    "start": "yarn serve",
    "build": "ng build --deploy-url='/static/'",
    "watch": "ng build --watch",
    "test": "ng test --watch=false",
    "prebuild": "node ./build/build-pre.js",
    "preserve": "yarn prebuild",
    "serve": "ng serve --proxy-config ../proxy.conf.json",
    "serve:en": "ng serve --proxy-config ../proxy.conf.json --configuration=en",
    "stop": "lsof -t -i tcp:4200 | xargs kill -9 || echo \"not running\"",
    "pretest": "yarn prebuild",
    "watch-test": "ng test --watch=true",
    "lint": "ng lint",
    "codegen": "yarn graphql-codegen --verbose --watch",
    "codegen-once": "yarn graphql-codegen"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.16",
    "@angular/common": "^19.2.16",
    "@angular/compiler": "^19.2.16",
    "@angular/core": "^19.2.16",
    "@angular/forms": "^19.2.16",
    "@angular/platform-browser": "^19.2.16",
    "@angular/platform-browser-dynamic": "^19.2.16",
    "@angular/router": "^19.2.16",
    "@apollo/client": "^3.0.0",
    "@fortawesome/angular-fontawesome": "~0.15.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@popperjs/core": "^2.11.8",
    "apollo-angular": "^7.0.0",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "colors": "^1.4.0",
    "graphql": "^16",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "underscore": "^1.13.6",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular-eslint/builder": "^18.2",
    "@angular-eslint/eslint-plugin": "^18.2",
    "@angular-eslint/eslint-plugin-template": "^18.2",
    "@angular-eslint/schematics": "^18.2",
    "@angular-eslint/template-parser": "^18.2",
    "@angular/cli": "~19.2.19",
    "@angular/compiler-cli": "^19.2.16",
    "@angular/localize": "^19.2.16",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@parcel/watcher": "^2.4.1",
    "@types/jasmine": "~4.3.0",
    "@types/underscore": "^1.11.15",
    "@typescript-eslint/eslint-plugin": "^8.0",
    "@typescript-eslint/parser": "^8.0",
    "eslint": "^9.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.5.4"
  }
}
